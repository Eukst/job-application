    <h1>Jobs Created</h1>
 
<%= paginate @jobs%>
<table class="table">
  <thead>
    <tr>
     <th>User ID</th>
      <th>Job title</th>
      <th>Experience</th>
      <th>Salary</th>
      
        <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @jobs.each do |job| %>
      <tr>
       <td><%= job.user_id %></td>
        <td><%= job.job_title %></td>
        <td><%= job.experience %></td>
        <td><%= job.salary %></td>
       
         <td><%= job.description %></td>
        <td><%= link_to 'Show', job %></td>
        <td><%= link_to 'Edit', edit_job_path(job) %></td>
        <td><%= link_to 'Destroy', job, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= paginate @jobs %>

