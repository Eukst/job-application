    <h1>Jobs Created</h1>
<% @jobs = Job.all %> <!-- for notes purpose -->
<%@jobs = Kaminari.paginate_array(@jobs).page(params[:page]).per(3)%>
<%= paginate @jobs %>
<table>
  <thead>
    <tr>
      <th>Job title</th>
      <th>Experience</th>
      <th>Salary</th>
       <th>User ID</th>
        <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @jobs.each do |job| %>
      <tr>
        <td><%= job.job_title %></td>
        <td><%= job.experience %></td>
        <td><%= job.salary %></td>
        <td><%= job.user_id %></td>
         <td><%= job.description %></td>
        <td><%= link_to 'Show', job %></td>
        <td><%= link_to 'Edit', edit_job_path(job) %></td>
        <td><%= link_to 'Destroy', job, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= paginate @jobs %>
    <h1>User Created</h1>
<% @users = User.all %> <!-- for notes purpose -->
<%@users = Kaminari.paginate_array(@users).page(params[:page]).per(5)%>
<%= paginate @users %>
<table>
  <thead>
    <tr>
      <th>Job title</th>
      <th>Experience</th>
      <th>Salary</th>
       <th>User ID</th>
        <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.first_name %></td>
        <td><%= user.last_name %></td>
        <td><%= user.email %></td>
        <td><%= user.address %></td>
         <td><%= user.city %></td>
        <td><%= link_to 'Show', user_profile_path(user.id) %></td>
        <td><%= link_to 'Edit', edit_user_path(user) %></td>
        <td><%= link_to 'Destroy', user_profile_path(user.id), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= paginate @users %>
