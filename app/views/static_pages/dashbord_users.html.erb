
<h1>User Created</h1>
<%= paginate @users %>
<table>
  <thead>
    <tr>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Email</th>
       <th>Address</th>
        <th>City</th>
        <th>User Role</th>
      <th colspan="3"> Options</th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.first_name %></td>
        <td><%= user.last_name %></td>
        <td><%= user.email %></td>
        <td><%= user.address %></td>
         <td><%= user.city %></td>
           <td><%= user.user_role %></td>
        <td><%= link_to 'Show', user_profile_path(user.id) %></td>
        <td><%= link_to 'Destroy', user_profile_path(user.id), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= paginate @users %>

<%= link_to 'Back To Home', root_path %>
